import ds.helper_functions as hf
import pandas as pd

conn1 = hf.connect(azure=True, db='TEPEZZA')
conn2 = hf.connect(azure=True, db='hdp_hub')

hcp = pd.read_sql("""SELECT [hcp_id] as hcp_id
      ,[npi]
      ,[first_name]
      ,[last_name]
      ,[specialty_group]
      ,[address_1]
      ,[address_2]
      ,[city]
      ,[state]
      ,[zip] as hcp_zip
  FROM [analytics].[mstr_hcp]""", conn1)

print(hcp)
